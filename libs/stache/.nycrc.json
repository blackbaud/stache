{
  "extends": "@istanbuljs/nyc-config-typescript",
  "extension": ".ts",
  "include": ["schematics/**/*.ts"],
  "exclude": [
    "**/*.spec.ts",
    "**/noop/**",
    "**/testing/**",
    "**/utility/json-file.ts"
  ],
  "all": true,
  "check-coverage": true,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "statements": 100,
  "reporter": ["text", "html", "json"],
  "report-dir": "../../../coverage/libs/stache-schematics",
  "temp-dir": "../../../.nyc_output"
}
